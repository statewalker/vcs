{
  "name": "@webrun-vcs/vcs",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./interfaces": {
      "types": "./dist/interfaces/index.d.ts",
      "import": "./dist/esm/interfaces/index.js",
      "require": "./dist/cjs/interfaces/index.cjs"
    },
    "./engine": {
      "types": "./dist/engine/index.d.ts",
      "import": "./dist/esm/engine/index.js",
      "require": "./dist/cjs/engine/index.cjs"
    },
    "./base": {
      "types": "./dist/base/index.d.ts",
      "import": "./dist/esm/base/index.js",
      "require": "./dist/cjs/base/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && rolldown -c && tsc --emitDeclarationOnly --declaration",
    "test": "vitest run --passWithNoTests",
    "lint": "biome lint src tests"
  },
  "dependencies": {
    "@webrun-vcs/utils": "workspace:*"
  },
  "devDependencies": {
    "@webrun-vcs/testing": "workspace:*",
    "@types/node": "catalog:",
    "rolldown": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
