{
  "name": "@webrun-vcs/vcs",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./interfaces": {
      "types": "./dist/interfaces/index.d.ts",
      "import": "./dist/esm/interfaces/index.js",
      "require": "./dist/cjs/interfaces/index.cjs"
    },
    "./format": {
      "types": "./dist/format/index.d.ts",
      "import": "./dist/esm/format/index.js",
      "require": "./dist/cjs/format/index.cjs"
    },
    "./binary-storage": {
      "types": "./dist/binary-storage/index.d.ts",
      "import": "./dist/esm/binary-storage/index.js",
      "require": "./dist/cjs/binary-storage/index.cjs"
    },
    "./object-storage": {
      "types": "./dist/object-storage/index.d.ts",
      "import": "./dist/esm/object-storage/index.js",
      "require": "./dist/cjs/object-storage/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && rolldown -c && tsc --emitDeclarationOnly --declaration",
    "test": "vitest run --passWithNoTests",
    "lint": "biome lint src tests"
  },
  "dependencies": {
    "@webrun-vcs/core": "workspace:*",
    "@webrun-vcs/utils": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "rolldown": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
