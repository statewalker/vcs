{
  "name": "@webrun-vcs/storage-sql",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./adapters/sql-js": {
      "types": "./dist/adapters/sql-js-adapter.d.ts",
      "import": "./dist/esm/adapters/sql-js-adapter.js",
      "require": "./dist/cjs/adapters/sql-js-adapter.cjs"
    }
  },
  "scripts": {
    "build": "rm -rf dist && rolldown -c && tsc --emitDeclarationOnly --declaration",
    "test": "vitest run",
    "lint": "biome lint src tests"
  },
  "dependencies": {
    "@webrun-vcs/common": "workspace:*",
    "@webrun-vcs/storage": "workspace:*",
    "@webrun-vcs/storage-default": "workspace:*"
  },
  "devDependencies": {
    "@webrun-vcs/storage-tests": "workspace:*",
    "sql.js": "^1.12.0",
    "@types/sql.js": "^1.4.9",
    "rolldown": "catalog:",
    "vitest": "catalog:",
    "typescript": "catalog:",
    "@types/node": "catalog:"
  },
  "peerDependencies": {
    "sql.js": ">=1.8.0"
  },
  "peerDependenciesMeta": {
    "sql.js": {
      "optional": true
    }
  }
}
