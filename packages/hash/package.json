{
  "name": "@webrun-vcs/hash",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./crc32": {
      "types": "./dist/crc32/index.d.ts",
      "import": "./dist/esm/crc32/index.js",
      "require": "./dist/cjs/crc32/index.cjs"
    },
    "./sha1": {
      "types": "./dist/sha1/index.d.ts",
      "import": "./dist/esm/sha1/index.js",
      "require": "./dist/cjs/sha1/index.cjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/esm/utils/index.js",
      "require": "./dist/cjs/utils/index.cjs"
    },
    "./fossil-checksum": {
      "types": "./dist/fossil-checksum/index.d.ts",
      "import": "./dist/esm/fossil-checksum/index.js",
      "require": "./dist/cjs/fossil-checksum/index.cjs"
    },
    "./rolling-checksum": {
      "types": "./dist/rolling-checksum/index.d.ts",
      "import": "./dist/esm/rolling-checksum/index.js",
      "require": "./dist/cjs/rolling-checksum/index.cjs"
    },
    "./strong-checksum": {
      "types": "./dist/strong-checksum/index.d.ts",
      "import": "./dist/esm/strong-checksum/index.js",
      "require": "./dist/cjs/strong-checksum/index.cjs"
    }
  },
  "scripts": {
    "build": "rm -rf dist && rolldown -c && tsc --emitDeclarationOnly --declaration",
    "test": "vitest run",
    "lint": "biome lint src tests"
  },
  "devDependencies": {
    "rolldown": "catalog:",
    "vitest": "catalog:",
    "typescript": "catalog:",
    "@types/node": "catalog:"
  }
}
