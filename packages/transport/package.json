{
  "name": "@statewalker/vcs-transport",
  "version": "0.1.0",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./protocol": {
      "types": "./dist/protocol/index.d.ts",
      "import": "./dist/protocol/index.js"
    },
    "./negotiation": {
      "types": "./dist/negotiation/index.d.ts",
      "import": "./dist/negotiation/index.js"
    },
    "./connection": {
      "types": "./dist/connection/index.d.ts",
      "import": "./dist/connection/index.js"
    },
    "./operations": {
      "types": "./dist/operations/index.d.ts",
      "import": "./dist/operations/index.js"
    },
    "./streams": {
      "types": "./dist/streams/index.d.ts",
      "import": "./dist/streams/index.js"
    },
    "./ports": {
      "types": "./dist/ports/index.d.ts",
      "import": "./dist/ports/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && rolldown -c && tsc --emitDeclarationOnly --declaration",
    "test": "vitest run",
    "lint": "biome lint src tests"
  },
  "dependencies": {
    "@statewalker/vcs-core": "workspace:*",
    "@statewalker/vcs-utils": "workspace:*"
  },
  "devDependencies": {
    "rolldown": "catalog:",
    "vitest": "catalog:",
    "typescript": "catalog:",
    "@types/node": "catalog:"
  }
}
