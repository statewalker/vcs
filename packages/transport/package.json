{
  "name": "@statewalker/vcs-transport",
  "version": "0.1.0",
  "description": "Git transport protocol implementation (fetch, push, clone over HTTP/WebSocket/WebRTC)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/statewalker/vcs.git",
    "directory": "packages/transport"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./protocol": {
      "types": "./dist/protocol/index.d.ts",
      "import": "./dist/protocol/index.js"
    },
    "./operations": {
      "types": "./dist/operations/index.d.ts",
      "import": "./dist/operations/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && rolldown -c && tsc --emitDeclarationOnly --declaration",
    "test": "vitest run",
    "lint": "biome lint src tests"
  },
  "dependencies": {
    "@statewalker/vcs-core": "workspace:*",
    "@statewalker/vcs-utils": "workspace:*"
  },
  "devDependencies": {
    "rolldown": "catalog:",
    "vitest": "catalog:",
    "typescript": "catalog:",
    "@types/node": "catalog:"
  }
}
