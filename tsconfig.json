{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@webrun-vcs/commands": ["packages/commands/src"],
      "@webrun-vcs/commands/*": ["packages/commands/src/*"],
      "@webrun-vcs/core": ["packages/core/src"],
      "@webrun-vcs/core/*": ["packages/core/src/*"],
      "@webrun-vcs/sandbox": ["packages/sandbox/src"],
      "@webrun-vcs/sandbox/*": ["packages/sandbox/src/*"],
      "@webrun-vcs/storage-tests": ["packages/storage-tests/src"],
      "@webrun-vcs/storage-tests/*": ["packages/storage-tests/src/*"],
      "@webrun-vcs/store-kv": ["packages/store-kv/src"],
      "@webrun-vcs/store-kv/*": ["packages/store-kv/src/*"],
      "@webrun-vcs/store-mem": ["packages/store-mem/src"],
      "@webrun-vcs/store-mem/*": ["packages/store-mem/src/*"],
      "@webrun-vcs/store-sql": ["packages/store-sql/src"],
      "@webrun-vcs/store-sql/*": ["packages/store-sql/src/*"],
      "@webrun-vcs/testing": ["packages/testing/src"],
      "@webrun-vcs/testing/*": ["packages/testing/src/*"],
      "@webrun-vcs/transport": ["packages/transport/src"],
      "@webrun-vcs/transport/*": ["packages/transport/src/*"],
      "@webrun-vcs/utils": ["packages/utils/src"],
      "@webrun-vcs/utils/*": ["packages/utils/src/*"]
    }
  },
  "include": [],
  "references": [
    { "path": "packages/utils" },
    { "path": "packages/core" },
    { "path": "packages/transport" },
    { "path": "packages/testing" },
    { "path": "packages/store-kv" },
    { "path": "packages/sandbox" },
    { "path": "packages/store-mem" },
    { "path": "packages/store-sql" },
    { "path": "packages/storage-tests" },
    { "path": "packages/commands" }
  ]
}
