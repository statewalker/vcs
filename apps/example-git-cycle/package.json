{
  "name": "@webrun-vcs/example-git-cycle",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Example application demonstrating the complete Git workflow with webrun-vcs",
  "scripts": {
    "start": "tsx src/main.ts",
    "step:01": "tsx src/steps/01-init-repository.ts",
    "step:02": "tsx src/steps/02-create-files.ts",
    "step:03": "tsx src/steps/03-build-trees.ts",
    "step:04": "tsx src/steps/04-create-commits.ts",
    "step:05": "tsx src/steps/05-update-files.ts",
    "step:06": "tsx src/steps/06-view-history.ts",
    "step:07": "tsx src/steps/07-restore-version.ts",
    "step:08": "tsx src/steps/08-branches-tags.ts",
    "all": "tsx src/main.ts"
  },
  "dependencies": {
    "@statewalker/webrun-files": "catalog:",
    "@webrun-vcs/core": "workspace:*",
    "@webrun-vcs/utils": "workspace:*",
    "@webrun-vcs/vcs": "workspace:*",
    "@webrun-vcs/storage-git": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:"
  }
}
